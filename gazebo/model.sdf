<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="Kratos">
   
   <static>false</static>
   <link name='chassis'>
    <pose>0 -0.33 0 0 0 0</pose>

    <inertial>
      <pose>0 0.5 0.1 0 -0 0</pose>
      <mass>10</mass>
    </inertial>

    <collision name='collision'>
      <geometry>
        <mesh>
          <scale>0.001 0.001 0.001</scale> <!-- the DAE is in millimeters. Gazebo is in meters. -->
          <uri>model://gazebo/Prometheus.dae</uri>
        </mesh>
      </geometry>
    </collision>

    <visual name='visual'>
      <geometry>
        <mesh>
          <scale>0.001 0.001 0.001</scale>
          <uri>model://gazebo/Prometheus.dae</uri>
        </mesh>
      </geometry>
    </visual>

    <collision name='caster_collision'>
      <pose>0 1.05 -0.057 0 0 0</pose>
      <geometry>
        <sphere>
          <radius>.05</radius>
        </sphere>
      </geometry>

      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster_visual'>
      <pose>0 1.05 -0.057 0 0 0</pose>
      <geometry>
        <sphere>
          <radius>.05</radius>
        </sphere>
      </geometry>
    </visual>

    <visual name='camra_visual'>
      <pose>0 1.5 0.1 0 0 0</pose>
      <geometry>
        <sphere>
          <radius>.05</radius>
        </sphere>
      </geometry>
    </visual>

    <sensor type="depth" name="xtion_sensor">
      <pose>0 1 0.1 0 0 1.57</pose>
      <always_on>true</always_on>
      <update_rate>20.0</update_rate>
      <topic>tteesstt</topic>
      <camera>
        <horizontal_fov>1.047</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.05</near>
          <far>5</far>
        </clip>
      </camera>
      <visualize>true</visualize>
    
      <!-- <plugin name='libkratos_camera' filename='libkratos_camera.so'/>
      <plugin name="libkratos_camera" filename="libkratos_camera.so"/> -->
    </sensor>

  </link>



  <link name="left_wheel">
    <pose>0.31 0.0 0.05 0 1.5707 0</pose>
    <collision name="collision">
      <geometry>
        <cylinder>
          <radius>.157</radius>
          <length>.065</length>
        </cylinder>
      </geometry>

      <surface>
        <friction>
          <ode>
            <mu>100.0</mu>
            <mu2>100.0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>

    </collision>
    <visual name="visual">
      <geometry>
        <cylinder>
          <radius>.157</radius>
          <length>.065</length>
        </cylinder>
      </geometry>
    </visual>
  </link>

  <link name="right_wheel">
    <pose>-0.31 0.0 0.05 0 1.5707 0</pose>
    <collision name="collision">
      <geometry>
        <cylinder>
          <radius>.157</radius>
          <length>.065</length>
        </cylinder>
      </geometry>

      <surface>
        <friction>
          <ode>
            <mu>100.0</mu>
            <mu2>100.0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>

    </collision>
    <visual name="visual">
      <geometry>
        <cylinder>
          <radius>.157</radius>
          <length>.065</length>
        </cylinder>
      </geometry>
    </visual>
  </link>

  <joint type="revolute" name="left_wheel_hinge">
    <pose>0 0 -0.03 0 0 0</pose>
    <child>left_wheel</child>
    <parent>chassis</parent>
    <axis>
      <xyz>1 0 0</xyz>
      <dynamics>
        <friction>1.0</friction>
        <damping>1.0</damping>
      </dynamics>
    </axis>
  </joint>

  <joint type="revolute" name="right_wheel_hinge">
    <pose>0 0 0.03 0 0 0</pose>
    <child>right_wheel</child>
    <parent>chassis</parent>
    <axis>
      <xyz>1 0 0</xyz>
      <dynamics>
        <friction>1.0</friction>
        <damping>1.0</damping>
      </dynamics>
    </axis>
  </joint>




